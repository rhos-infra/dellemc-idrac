	push = group nfv-qe-core
	push = group user/Vadim Khitrin (vkhitrin)
	read = group nfv-qe-core
	read = group user/Vadim Khitrin (vkhitrin)
	rebase = group nfv-qe-core
	rebase = group user/Vadim Khitrin (vkhitrin)
	removeReviewer = group nfv-qe-core
	removeReviewer = group user/Vadim Khitrin (vkhitrin)
	submit = group nfv-qe-core
	submit = group user/Vadim Khitrin (vkhitrin)
[access "refs/heads/*"]
	create = group nfv-qe-core
	create = group user/Vadim Khitrin (vkhitrin)
	push = group nfv-qe-core
